# Cloudflare Workers Environment Variables
# Copy this to .env.workers and update with your values

# Supabase Configuration
SUPABASE_URL=your-supabase-url
SUPABASE_ANON_KEY=your-supabase-anon-key

# OpenAI API (if used)
OPENAI_API_KEY=your-openai-api-key

# Environment Configuration
ENVIRONMENT=production
NODE_ENV=production

# Custom Domain (production only)
CUSTOM_DOMAIN=app.battleborncapital.com

# Analytics (optional)
ANALYTICS_DATASET=battleborn_analytics

# SMTP Configuration (if needed for server-side emails)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-app-password