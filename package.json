{
  "name": "tax-calculator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.6.7"
  },
  "scripts": {
    "dev": "vite",
    "dev:debug": "VITE_ENABLE_DEBUG_LOGS=true vite",
    "build": "tsc && vite build",
    "build:workers": "npm run build",
    "dev:workers": "npx wrangler dev",
    "deploy:workers": "npx wrangler deploy --env production",
    "build:dev": "NODE_ENV=development tsc && vite build --mode development",
    "build:prod": "NODE_ENV=production tsc && vite build --mode production",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "preview:prod": "vite preview --mode production",
    "postinstall": "npx update-browserslist-db@latest",
    "test": "jest",
    "test:epic1": "node scripts/run-epic1-tests.js",
    "test:epic2": "jest src/tests/epic2-functionality.test.ts --testTimeout=30000",
    "test:epic2:functionality": "jest src/tests/epic2-functionality.test.ts --testTimeout=30000",
    "test:security": "jest src/tests/epic1-security.test.ts --testTimeout=30000",
    "test:integration": "jest src/tests/epic1-integration.test.ts --testTimeout=30000",
    "test:performance": "jest src/tests/epic1-performance.test.ts --testTimeout=30000",
    "test:epic1:coverage": "jest src/tests/epic1-*.test.ts --coverage --testTimeout=30000",
    "test:epic2:coverage": "jest src/tests/epic2-*.test.ts --coverage --testTimeout=30000",
    "test:all": "jest --testTimeout=30000",
    "test:coverage": "jest --coverage --testTimeout=30000",
    "test:watch": "jest --watch",
    "setup:env": "bash setup-environments.sh",
    "db:migrate": "node run_migration.js",
    "db:test": "node test_db_schema.js",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist build .vite",
    "pdf-server": "node pdf-server.cjs",
    "dev:full": "concurrently \"npm run dev\" \"npm run pdf-server\""
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@headlessui/react": "^2.2.2",
    "@heroicons/react": "^2.2.0",
    "@mui/material": "^7.1.1",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@react-google-maps/api": "^2.19.3",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/ssr": "^0.1.0",
    "@supabase/supabase-js": "^2.53.0",
    "@tremor/react": "^3.18.7",
    "@types/nodemailer": "^6.4.17",
    "@types/papaparse": "^5.3.16",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-window": "^1.8.8",
    "@types/styled-components": "^5.1.34",
    "chart.js": "^4.4.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "framer-motion": "^11.18.2",
    "html2canvas": "^1.4.1",
    "immer": "^10.0.4",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.344.0",
    "nodemailer": "^6.10.1",
    "openai": "^5.8.2",
    "papaparse": "^5.5.2",
    "puppeteer": "^24.15.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-google-autocomplete": "^2.7.3",
    "react-number-format": "^5.3.1",
    "react-router-dom": "^6.22.1",
    "react-signature-canvas": "^1.0.7",
    "react-toastify": "^11.0.5",
    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^1.0.10",
    "recharts": "^2.15.3",
    "styled-components": "^6.1.17",
    "tailwind-merge": "^3.3.0",
    "uuid": "^11.1.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.27.1",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.27.1",
    "@babel/preset-env": "^7.27.1",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/runtime": "^7.27.1",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/jspdf": "^1.3.3",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/react-signature-canvas": "^1.0.7",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "babel-jest": "^29.7.0",
    "concurrently": "^9.2.0",
    "dotenv": "^16.5.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "glob": "^10.3.10",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.33",
    "react-hot-toast": "^2.5.2",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.3.2",
    "ts-jest-mock-import-meta": "^1.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^5.0.12",
    "wrangler": "^4.25.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
